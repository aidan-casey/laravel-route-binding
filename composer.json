{
    "name": "aidan-casey/laravel-route-binding",
    "description": "Adds Laravel route binding for non-controller objects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Casey",
            "email": "aidan.casey@anteris.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AidanCasey\\Laravel\\RouteBinding\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AidanCasey\\Laravel\\RouteBinding\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "coverage": "vendor/bin/phpunit --coverage-html build/coverage"
    }
}
